@model List<ContractModel>
<div>
    @if (Model.Count > 0)
    {
        <h5><i>@T("appwork.contracts.recentlycontract")</i></h5>
        <div class="row">
            @{ foreach (ContractModel ct in Model)
                {
                    @await Html.PartialAsync("_ContractViewInfo", ct)
                }
            }
        </div>
    }
</div>