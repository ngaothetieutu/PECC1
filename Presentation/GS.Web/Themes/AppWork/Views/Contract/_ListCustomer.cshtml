@Model List<ContractResourceModel>
<ul class="list-group list-group-flush">
    @if (Model.Count > 0)
    {
        foreach (var m in Model)
        {
                <li class="list-group-item">
                    <div class="media align-items-center">
                        <img src="@m.CustomerAvatarUrl" class="d-block ui-w-30 rounded-circle" alt="">
                        <div class="media-body px-2">
                            <a href="javascript:void(0)" class="text-dark">@m.CustomerFullName</a>
                        </div>
                        <a href="javascript:void(0)" class="d-block text-light text-large font-weight-light" data-toggle="modal" data-target="#tabContractResourceAdd" data-id="@m.CustomerId">
                            <i class="lnr lnr-pencil d-block"></i>
                        </a>
                    </div>
                </li>
        }
    }
    else
    {
        <li class="list-group-item">
            <div class="media align-items-center">

            </div>
        </li>
    }
</ul>